/**index.wxss**/
.listContent {
  min-height: 100vh;
  background: #F5F7FA;
  padding: 30rpx 0 0;
  box-sizing: border-box;
  overflow-y: scroll;
}
.container{
    width: 100%;
    padding: 0 30rpx;
    box-sizing: border-box;
    background: #fff;
    margin-bottom: 30rpx;
}
.listBox{
    width: 100%;
    font-size: 28rpx;
    color: #586471;
    padding: 30rpx 0;
    box-sizing: border-box;
    
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid #E4EAF2;
}
.listBox .left .title{
    font-size: 30rpx;
    color: #293D53;
    height: 76rpx;
    line-height: 38rpx;
    overflow:hidden; 
    text-overflow:ellipsis;
    display:-webkit-box; 
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2; 
    margin-bottom: 40rpx;
}
.listBox .left .time{
    font-size: 24rpx;
    color: #586471;
    line-height: 43rpx;
}
.cover{
    flex: 0 0 274rpx;
    height: 140rpx;
    margin-left: 22rpx;
}
.cover image{
    width: 100%;
    height: 100%;
    border-radius: 12rpx;
}
.container :last-child{
    border-bottom: 0;
}
